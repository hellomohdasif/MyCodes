// delete all post attachment from wordpress


1) 
DELETE FROM wp_postmeta
WHERE post_id IN
(
SELECT id
FROM wp_posts
WHERE post_type = 'attachment'
)
;
 
2)  DELETE FROM wp_posts WHERE post_type = 'attachment'

